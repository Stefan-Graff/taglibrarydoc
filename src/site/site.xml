<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns="http://maven.apache.org/DECORATION/1.8.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 https://maven.apache.org/xsd/decoration-1.8.0.xsd"
    name="taglibrarydoc: Tag Library Documentation Generator ${project.version}">

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.11.0</version>
    </skin>

    <custom>
        <fluidoSkin>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <skipGenerationDate>true</skipGenerationDate>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <gitHub>
                <projectId>ste-gr/tlddoc</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>

    <body>
        <menu name="TLDDoc">
            <item name="Welcome" href="index.html" />
            <item name="Tag Library Repository" href="repository.html" />
            <item name="Screenshot" href="screenshot.html" />
            <item name="Download and Installation" href="download.html" />
        </menu>

        <menu name="Documentation">
            <item name="Features" href="features.html" />
            <item name="Building From Source" href="building.html" />
            <item name="Usage Instructions" href="usage.html" />
            <item name="Support" href="support.html" />
            <item name="FAQs" href="faq.html" />
            <item name="Revision History" href="changes-report.html" />
            <item name="Javadoc" href="apidocs/index.html" />
        </menu>

        <menu ref="reports" />

    </body>
</project>